#include<iostream>
using namespace std;
#include<bits/stdc++.h>

int main(){
   ios_base::sync_with_stdio(false);
   cin.tie(0);
   stack< int >st;
    int T ;
    cin>>T ;
   while( T--){
      int t; cin>>t;
      if(t==1){
            int a; cin>>a;
            st.push(a);
   }
    else if( t == 2){
           if( ! st.empty() )
                        st.pop();

   }else {
       if(st.empty())
                 cout<<"Empty!\n";
       else cout<<st.top()<<"\n";
   }

}
}
